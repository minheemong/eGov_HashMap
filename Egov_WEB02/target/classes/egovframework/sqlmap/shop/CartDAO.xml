<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ezen.shop.dao.CartDAO">

	<insert id="cartInsert" statementType="CALLABLE" parameterType="java.util.HashMap">
		{CALL cartInsert( #{PSEQ}, #{QUANTITY}, #{ID} )}
	</insert> 
	
	<select id="cartList" statementType="CALLABLE" parameterType="java.util.HashMap">
		{CALL cartList( #{id},
		#{ref_cursor, mode=OUT, jdbcType=CURSOR, javaType=ResultSet, resultMap=shopMap} )}
	</select> 
	
	<delete id="cartDelete" statementType="CALLABLE" parameterType="java.util.HashMap">
		{CALL cartDelete( #{cseq} )}
	</delete>
	
</mapper>